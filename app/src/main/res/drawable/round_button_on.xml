<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">

    <item android:state_pressed="false">
        <shape android:shape="oval">
            <gradient android:centerColor="#05e73d" android:endColor="#04a62f" android:startColor="#049f16" />
        </shape>
    </item>
    <item android:state_pressed="true">
        <shape android:shape="oval">
            <gradient android:centerColor="#d91b6a" android:endColor="#b30528" android:startColor="#b10626" />
        </shape>
    </item>
</selector>