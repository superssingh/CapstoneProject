<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">

    <item android:state_pressed="false">
        <shape android:shape="oval">
            <gradient android:centerColor="#07a90f" android:endColor="#08c935" android:startColor="#06c629" />
        </shape>
    </item>
    <item android:state_pressed="true">
        <shape android:shape="oval">
            <gradient android:centerColor="#0af5ee" android:endColor="#05b3b0" android:startColor="#06b1ab" />
        </shape>
    </item>
</selector>